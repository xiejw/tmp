cmake \
        -DCMAKE_POSITION_INDEPENDENT_CODE=TRUE \
        -B ../build -S . \
        -DCMAKE_INSTALL_PREFIX=$HOME/Workspace/build/usr  \
        -DBUILD_SHARED_LIBS=FALSE -DCMAKE_BUILD_TYPE=Release

make -C ../build
make -C ../build install


